<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="view_appraisal_generate_form" model="ir.ui.view">
		<field name="name">appraisal.generate.form</field>
		<field name="model">appraisal.generate</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group string="Employees">
						<field name="employee_ids" nolabel="1"  />
					</group>
					<footer>
						<button name="generate" type="object" string="Generate" class="btn-primary" />
						<button special="cancel" string="Cancel" class="btn-default" />
					</footer>
				</sheet>
			</form>
		</field>
	</record>


</odoo>